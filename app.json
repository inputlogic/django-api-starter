{
  "name": "Django API Starter",
  "env": {
    "DJANGO_ENV": {
      "description": "Sets Django run environment",
      "value": "staging"
    }
  },
  "addons": ["heroku-postgresql:hobby-dev"],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby",
      "type": "web"
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
